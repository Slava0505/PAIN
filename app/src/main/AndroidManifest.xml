<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="codestart.info.kotlinphoto">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.example.pain_1.unsarpMasking">
        </activity>
        <activity
                android:name="com.example.pain_1.RetouchActivity"
                android:label="@string/title_activity_retouch"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="com.example.pain_1.ResizeActivity">
        </activity>
        <activity android:name="com.example.pain_1.RotateActivity">
        </activity>
        <activity
                android:name="com.example.pain_1.SplinesActivity"
                android:label="@string/title_activity_splines"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="com.example.pain_1.BilinearActivity">
        </activity>
        <activity
                android:name="com.example.pain_1.FilterActivity"
                android:label="@string/title_activity_filter">
        </activity>
        <activity android:name="com.example.pain_1.EditorActivity">
        </activity>
        <activity android:name="com.example.pain_1.AStarActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>